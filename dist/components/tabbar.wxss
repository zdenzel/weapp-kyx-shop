.tabbar{width:100%;height:100rpx;white-space:nowrap;position:relative;left:0;top:0;z-index:10;background-image:url('https://api.keyixing.com/applet/images/bg.jpg');background-color:#ddd4c1;text-align:center}.tabbar:after{content:'';position:absolute;bottom:0;left:0;width:100%;height:2rpx;background-color:#b9ae8b}.tabbar-contain{display:inline-block;padding:0 10rpx;position:relative}.tabbar-contain.is-flex{display:flex}.tabbar-contain.is-flex .tabbar-button{flex:1;display:block}.tabbar-contain.is-flex .tabbar-button:after{content:'';height:1em;width:3rpx;margin-top:-0.5em;position:absolute;left:0;top:50%;background-color:#b9ae8b}.tabbar-contain.is-flex .tabbar-button:first-child:after{display:none}.tabbar-button{display:inline-block;height:100rpx;line-height:100rpx;padding:0 10rpx;position:relative;font-size:36rpx}.tabbar-button text{display:block;overflow:hidden;position:relative;padding:0 10rpx;text-overflow:ellipsis;white-space:nowrap}.tabbar-button text:after{display:none;content:'';position:absolute;bottom:0;left:0;width:100%;height:5rpx;background-color:#452713}.tabbar-button.active text:after{display:inherit}.tabbar-button .badge{min-width:20rpx;line-height:1;padding:3rpx 5rpx;position:absolute;right:5rpx;top:10rpx;text-align:center;background-color:#a91e11;font-size:24rpx;color:#e9e0cc;border-radius:20rpx}